{
  "name": "dobmilano",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=12000\" NEXT_DISABLE_SWC_MINIFY=1 NEXT_TELEMETRY_DISABLED=1 NEXT_DISABLE_ESLINT=1 next build --no-lint",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed:shop": "cross-env NODE_OPTIONS=--no-deprecation payload run src/seed/run-shop-seed.ts",
    "seed:map-products": "cross-env NODE_OPTIONS=--no-deprecation payload run src/seed/map-products-taxonomies.ts",
    "media:regenerate-sizes": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/regenerate-media-sizes.ts",
    "test:shop:webhook": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-webhook.ts",
    "test:shop:concurrency": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-concurrency.ts",
    "test:shop:cart-validation": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-cart-validation.ts",
    "test:shop:failed-payment": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-payment-failed.ts",
    "test:shop:admin-alignment": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-admin-alignment.ts",
    "test:shop:tamper": "cross-env NODE_OPTIONS=--no-deprecation payload run src/scripts/test-shop-tamper.ts",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.71.1",
    "@payloadcms/email-nodemailer": "3.71.1",
    "@payloadcms/next": "3.71.1",
    "@payloadcms/richtext-lexical": "3.71.1",
    "@payloadcms/ui": "3.71.1",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@tsamantanis/react-glassmorphism": "^1.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "framer-motion": "^12.26.2",
    "graphql": "^16.8.1",
    "next": "15.4.10",
    "nodemailer": "^7.0.12",
    "payload": "3.71.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "sharp": "0.34.5",
    "stripe": "20.3.1",
    "styled-components": "^6.3.8",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^7.0.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "nx": "22.3.3",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "nx": {}
}
